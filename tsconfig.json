{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "alwaysStrict": true,
        "sourceMap": true,
        "resolveJsonModule": true,
        "outDir": "./dist",
        "declaration": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "tools",
        "dist",
        "contracts",
        "test_integration",
        "test",
        "tools",
        "data_generator",
        "migrations"
    ]
}
